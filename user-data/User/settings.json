// 将设置放入此文件中以覆盖默认设置
{
    "editor.rulers": [80, 100],
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}",
    "path-intellisense.showHiddenFiles": true,
    "window.zoomLevel": 1,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "gitlens.keymap": "alternate",
    "editor.fontFamily": "'Source Code Pro Medium', Consolas, '微软雅黑', 'Courier New', monospace",
    "window.openFoldersInNewWindow": "on",
    "window.openFilesInNewWindow": "on",
    "editor.minimap.enabled": false,
    "editor.fontSize": 12,
    "workbench.editor.enablePreview": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "markdown-pdf.executablePath": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe",
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "remote.SSH.remotePlatform": {
        "vm-ubuntu18": "linux",
        "vm-ubuntu-server-20.04": "linux"
    },
    "search.useIgnoreFiles": false,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "type": "line",
            "foreground": "#ffffff",
            "iconColour": "#ffff00",
        },
        // "FIXME": {
        //     "foreground": "black",
        //     "iconColour": "yellow",
        //     "gutterIcon": true
        // }
    },
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|\\s*\\*|^)\\s*($TAGS):?|^\\s*- \\[ \\])",
    "workbench.editor.closeEmptyGroups": false,
    "workbench.startupEditor": "newUntitledFile",
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.formatting.provider": "black",
    "vsicons.dontShowNewVersionMessage": true,
    "licenser.disableAutoHeaderInsertion": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.banner": "always",
    "security.workspace.trust.startupPrompt": "always",
    "workbench.colorTheme": "Dracula At Night",
    "http.proxy": "http://127.0.0.1:20000",
    "shellformat.useEditorConfig": true,
    "shellformat.flag": "-kp -ci",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "redhat.telemetry.enabled": false,
    "C_Cpp.default.compilerPath": "C:\\Program Files\\Microsoft Visual Studio\\2022\\Community\\VC\\Tools\\MSVC\\14.33.31629\\bin\\Hostx64\\x64\\cl.exe",
    "licenser.author": "Canfoderiskii",
    "gitlens.codeLens.enabled": false,
	"update.enableWindowsBackgroundUpdates": false,
	"[python]": {
		"editor.formatOnType": true
	},
	"aiXcoder.searchIntegratedWindow": true
}